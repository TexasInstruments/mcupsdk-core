/**
 * Import the modules used in this configuration.
 */
const cmpss      = scripting.addModule("/drivers/cmpss/cmpss", {}, false);
const cmpss1     = cmpss.addInstance();
const gpio       = scripting.addModule("/drivers/gpio/gpio", {}, false);
const gpio1      = gpio.addInstance();
const gpio2      = gpio.addInstance();
const gpio3      = gpio.addInstance();
const uart       = scripting.addModule("/drivers/uart/uart", {}, false);
const uart1      = uart.addInstance();
const clock      = scripting.addModule("/kernel/dpl/clock");
const debug_log  = scripting.addModule("/kernel/dpl/debug_log");
const mpu_armv7  = scripting.addModule("/kernel/dpl/mpu_armv7", {}, false);
const mpu_armv71 = mpu_armv7.addInstance();
const mpu_armv72 = mpu_armv7.addInstance();
const mpu_armv73 = mpu_armv7.addInstance();
const mpu_armv74 = mpu_armv7.addInstance();

/**
 * Write custom configuration values to the imported modules.
 */
cmpss1.$name = "CONFIG_CMPSS0";

gpio1.$name                = "GPIO_INPUT";
gpio1.GPIO.gpioPin.$assign = "ball.D13";

gpio2.pinDir               = "OUTPUT";
gpio2.$name                = "GPIO_OUTPUT";
gpio2.GPIO.gpioPin.$assign = "ball.D7";

gpio3.$name                = "CMPSS";
gpio3.GPIO.gpioPin.$assign = "ball.C8";

uart1.$name            = "TESTER_UART";
uart1.UART.$assign     = "UART3";
uart1.UART.RXD.$assign = "SPI0_CS0";
uart1.UART.TXD.$assign = "SPI0_CLK";

debug_log.enableUartLog            = true;
